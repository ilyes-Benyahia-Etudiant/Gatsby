            <div class="card-body">
                <Card.Title>{{Data.cardTitle_2}}</Card.Title>
                <p class="card-text">{{Data.cardText_2}}</p>
                <Card.Text>{{Data.cardTarif_2}}</Card.Text>
            </div>
            </div>
            

            
            <div class="card" style="width: 40rem;">
            <img src={{Data.data_JusteMoi}} class="card-img-top" alt="picture">
            <div class="card-body">
                <Card.Title>{{Data.cardTitle_3}}</Card.Title>
                <p class="card-text">{{Data.cardText_3}}</p>
                <Card.Text>{{Data.cardTarif_3}}</Card.Text>
            </div>
            </div>
        
            
