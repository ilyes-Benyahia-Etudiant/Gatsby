            </div>
            

            
            <div class="card" style="width: 40rem;">
            <img src={{Data.data_Baby}} class="card-img-top" alt="picture">
            <div class="card-body">
                <Card.Title>{{Data.cardTitle_5}}</Card.Title>
                <p class="card-text">{{Data.cardText_5}}</p>
                <Card.Text>{{Data.cardTarif_5}}</Card.Text>
            </div>
            </div>
            
            
            
            <div class="card" style="width: 40rem;">
            <img src={{Data.data_Immortalize}} class="card-img-top" alt="picture">
            <div class="card-body">
                <Card.Title>{{Data.cardTitle_6}}</Card.Title>
                <p class="card-text">{{Data.cardText_6}}</p>
                <Card.Text>{{Data.cardTarif_6}}</Card.Text>
            </div>
            
        </div>
          
    </section>
<!-- End section Tarif/services -->



<!-- Start section Contact -->

    <section id='contact'>
        
            <div class='container-fluid'>
                <form class='contact-wrapper' action="https://formspree.io/f/mnqyzvrz" method="POST">
                <h4 id='title'>Contact<br></br> Quantin Photographe!</h4>
                        <label id="email">
                            
                            <input type="email" name="email" placeholder='Enter your email'>
                        </label>
                        <label id="message">
                            
                            <textarea name="message" placeholder='Ecrivez votre message'></textarea>
                        </label>
                        <!-- your other form fields go here -->
                        <button class='boutton' type="submit">Send</button>
                    </form>
            </div>
    </section>

<!-- End section Contact -->


<!-- Start Footer -->

    <footer class="footer bg-body-tertiary">
